	file = {Bryan et al. - 2007 - Resolution convergence and sensitivity studies wit.pdf:/home/lecorre/Zotero/storage/HYIUYLGY/Bryan et al. - 2007 - Resolution convergence and sensitivity studies wit.pdf:application/pdf}
}

@article{hecht,
