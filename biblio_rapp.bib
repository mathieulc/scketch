	year = {2002},
	pages = {21},
	file = {Cuny et al. - 2002 - Labrador Sea Boundary Currents and the Fate of the.pdf:/home/lecorre/Zotero/storage/9NMYTL4V/Cuny et al. - 2002 - Labrador Sea Boundary Currents and the Fate of the.pdf:application/pdf}
}

@article{holliday2009,
