	language = {en},
	urldate = {2019-04-24},
	journal = {Ocean Modelling},
	author = {Lemarié, Florian and Kurian, Jaison and Shchepetkin, Alexander F. and Jeroen Molemaker, M. and Colas, FranÃ§ois and McWilliams, James C.},
	month = jan,
	year = {2012},
	pages = {57--79},
	file = {Lemarié et al. - 2012 - Are there inescapable issues prohibiting the use o.pdf:/home/lecorre/Zotero/storage/KK8ZKP3G/LemariÃ© et al. - 2012 - Are there inescapable issues prohibiting the use o.pdf:application/pdf}
}

