	pages = {73--83},
	file = {Hughes - 2000 - A theoretical reason to expect inviscid western bo.pdf:/home/lecorre/Zotero/storage/Y7HF3RPW/Hughes - 2000 - A theoretical reason to expect inviscid western bo.pdf:application/pdf}
}

@article{jackson2006,
	title = {Topographic {Control} of {Basin} and {Channel} {Flows}: {The} {Role} of {Bottom} {Pressure} {Torques} and {Friction}},
